{
  "name": "emanapay-design-system",
  "version": "0.0.1",
  "description": "Design System for EmanaPay",
  "private": false,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "build": "tsc --project tsconfig.json",
    "clean": "rimraf dist",
    "test": "jest",
    "lint": "eslint .",
    "storybook": "STORYBOOK_ENABLED=true react-native start",
    "storybook:ios": "STORYBOOK_ENABLED=true react-native run-ios",
    "storybook:android": "STORYBOOK_ENABLED=true react-native run-android",
    "storybook:web": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "prepare": "npm run clean && npm run build"
  },
  "dependencies": {
    "react": "18.3.1",
    "react-native": "0.75.4",
    "react-native-gesture-handler": "^2.21.2",
    "react-native-mask-input": "^1.2.3",
    "react-native-reanimated": "^3.16.6",
    "react-native-svg": "^15.10.1",
    "react-native-vector-icons": "^10.2.0",
    "react-native-video": "^6.8.2",
    "react-native-safe-area-context": "^4.11.0"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-native": ">=0.70.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/preset-react": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/babel-preset": "0.75.3",
    "@react-native/eslint-config": "0.75.3",
    "@react-native/metro-config": "0.75.3",
    "@react-native/typescript-config": "0.75.3",
    "@storybook/addon-actions": "^8.4.7",
    "@storybook/addon-controls": "^8.4.7",
    "@storybook/addon-docs": "^8.4.7",
    "@storybook/addon-essentials": "^8.4.7",
    "@storybook/addon-links": "^8.4.7",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.9.0",
    "@types/jest": "^29.5.13",
    "@types/react": "^19.0.2",
    "@types/react-native": "^0.73.0",
    "babel-jest": "^29.6.3",
    "eslint": "^8.19.0",
    "jest": "^29.7.0",
    "typescript": "5.1.6"
  },
  "engines": {
    "node": ">=18"
  },
  "keywords": [
    "react-native",
    "design-system",
    "ui-library"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/seu-usuario/seu-repositorio.git"
  },
  "bugs": {
    "url": "https://github.com/seu-usuario/seu-repositorio/issues"
  },
  "homepage": "https://github.com/seu-usuario/seu-repositorio#readme",
  "license": "MIT",
  "packageManager": "yarn@4.5.0"
}
